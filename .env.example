OPENAI_API_KEY=your-openai-api-key-here

# Database configuration
POSTGRES_HOST=postgres
POSTGRES_PORT=5432
POSTGRES_DB=fastapi
POSTGRES_USER=fastapi
POSTGRES_PASSWORD=fastapi
WRITER_DB_URL=postgresql+asyncpg://fastapi:fastapi@postgres:5432/fastapi
READER_DB_URL=postgresql+asyncpg://fastapi:fastapi@postgres:5432/fastapi

# Environment
ENV=local

# AgentOS Configuration
AGNO_DEFAULT_MODEL=gpt-4o-mini

# WAHA Configuration
WAHA_API_URL=http://waha:3000/api
WAHA_SESSION_NAME=default
WAHA_LOG_FORMAT=JSON
WHATSAPP_HOOK_URL=http://api:8000/api/v1/waha/webhook
WHATSAPP_HOOK_EVENTS=message,session.status
WHATSAPP_HOOK_RETRIES_ATTEMPTS=4
WHATSAPP_HOOK_RETRIES_DELAY_SECONDS=3
WAHA_API_KEY=sha512:{your-waha-api-key}
